void bonAppetit(int bill_count, int* bill, int k, int b) {
    int i,wtot=0,wavg=0,tot=0,avg=0;
    for(i=0;i<bill_count;i++)
    {
        if(i!=k)
        {
            tot=tot+bill[i];
        }
    }
    if((tot/2)==b)
    {
        printf("Bon Appetit");
    }
    else
    {
        printf("%d",b-(tot/2));
    }


}
