int hurdleRace(int k, int height_count, int* height) {
    int max=0,i,temp=0;
    for(i=0;i<height_count;i++)
    {
        if(height[i]>max)
        {
            max=height[i];
        }
    }
        if(k<max)
        {
            temp=max-k;
            return temp;
        }
        else
        {
            return 0;
        }
}
